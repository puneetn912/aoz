    "angular-route": "^1.7.3",
    "aws-sdk": "^2.311.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "bcrypt": "^3.0.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.18.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "~1.4.3",
