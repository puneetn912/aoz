{
  "name": "caterer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "DEBUG=admin:* npm start",
    "debug-win": "SET DEBUG=admin:* & npm start",
    "dev": "NODE_ENV=dev npm run debug",
    "dev-debug-win": "SET NODE_ENV=dev & npm run debug-win",
    "stage-debug": "NODE_ENV=stage npm run debug",
    "stage-debug-win": "SET NODE_ENV=stage & npm run debug-win",
    "prod": "NODE_ENV=prod npm run debug",
    "prod-debug-win": "SET NODE_ENV=prod & npm run debug-win",
    "nodemon-start": "./node_modules/.bin/nodemon ./bin/www",
    "nodemon": "DEBUG=admin:* npm run nodemon-start",
    "nodemon-win": "SET DEBUG=admin:* & npm run nodemon-start",
    "dev-nodemon-win": "SET NODE_ENV=dev & npm run nodemon-win",
    "dev-nodemon": "NODE_ENV=dev npm run nodemon",
    "stage-nodemon-win": "SET NODE_ENV=stage & npm run nodemon-win",
    "stage-nodemon": "NODE_ENV=stage npm run nodemon",
    "prod-nodemon-win": "SET NODE_ENV=prod & npm run nodemon-win",
    "prod-nodemon": "NODE_ENV=prod npm run nodemon",
    "dev-webpack": "./node_modules/.bin/webpack --config webpack.dev.js --display-error-details --watch",
    "stage-webpack": "./node_modules/.bin/webpack --config webpack.stage.js --display-error-details --watch",
    "prod-webpack": "./node_modules/.bin/webpack --config webpack.prod.js",
    "webpack-dev": "./node_modules/.bin/webpack --mode development --watch",
    "webpack-prod": "./node_modules/.bin/webpack --mode production --watch"
  },
